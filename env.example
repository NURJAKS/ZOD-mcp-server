# Copy this file to .env and fill in your real values locally.
# Do NOT commit your real .env file.

# GitHub API Configuration
GITHUB_TOKEN=ghp_your_personal_access_token_here

# OpenRouter Configuration (for semantic search)
OPENROUTER_API_KEY=sk-or-your-openrouter-key

# Qdrant Vector Database
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your-qdrant-api-key

# Redis Cache
REDIS_URL=redis://localhost:6379

# Database
DATABASE_URL=sqlite://./data/nia.db

# Web Search APIs
SERPER_API_KEY=your-serper-key
SERPAPI_KEY=your-serpapi-key

# Rate Limiting
GITHUB_RATE_LIMIT=5000
OPENROUTER_RATE_LIMIT=100

# Indexing Settings
MAX_FILE_SIZE=1024000
MAX_REPOSITORY_SIZE=100000000
INDEXING_TIMEOUT=300000

